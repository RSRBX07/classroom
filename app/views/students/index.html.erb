<div class="container">

  <div class="jumbotron">
    <h1>Students#index</h1>
    <p>Here is the list of our students</p>
    <% @students.each do |student|  %>
      <%= student.name %>
      <%= student.avatar %>
      <%= image_tag(student.avatar, alt: student.name) %>
       <h2><%= link_to student.name, students_index_path(student) %></h2>
    <% end %>
  </div>
</div>
